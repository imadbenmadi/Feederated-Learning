{
    "global_update_interval_minutes": 1,
    "local_training_interval_seconds": 30,
    "min_devices_for_aggregation": 3,
    "auto_aggregation": {
        "enabled": true,
        "threshold": 3,
        "strategy": "fedavg"
    },
    "backup": {
        "enabled": true,
        "interval_hours": 6,
        "max_backups": 10
    },
    "scheduler": {
        "run_on_startup": true,
        "timezone": "UTC"
    }
}
